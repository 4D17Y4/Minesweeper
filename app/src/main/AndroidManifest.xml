        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".highScore_Activity"></activity>
        <activity android:name=".game_Activity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".info"
            android:theme="@style/AppTheme.CustomTheme"></activity>
        <activity android:name=".win"
            android:theme="@style/AppTheme.CustomTheme3">

        </activity>
        <activity android:name=".loose"
            android:theme="@style/AppTheme.CustomTheme3"></activity>

    </application>

</manifest>